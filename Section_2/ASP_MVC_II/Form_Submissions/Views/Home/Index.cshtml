@model Login
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Form Login</title>
</head>
<body>
    <h1>Log In</h1>
    
    <form asp-action="Login" asp-controller="Home" method='post'>
        <p>
        <span asp-validation-for = "FirstName"></span>
        <label asp-for="FirstName">FirstName</label>
        <input asp-for="FirstName" id="FirstName">
        </p>

        <p>
        <span asp-validation-for = "LastName"></span>
        <label asp-for="LastName">LastName</label>
        <input asp-for="LastName" id="LastName">
        </p>
        
        <p>
        <span asp-validation-for = "Age"></span>
        <label asp-for="Age">Age</label>
        <input asp-for="Age" id="Age">
        </p>
        
        <p>
        <span asp-validation-for = "Email"></span>
        <label asp-for="Email">Email</label>
        <input asp-for="Email" id="Email">
        </p>

        <p>
        <span asp-validation-for = "Password"></span>
        <label asp-for="Password">Password</label>
        <input asp-for="Password" id="Password">
        </p>

        <button type="submit">Submit</button>

        </form>
    </body>
</html>

