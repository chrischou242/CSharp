<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Here are some delicious foods</h1>
    @{
        List<string> Food = new List<string>
            {
                "Apple Pie",
                "Burriots",
                "Clams Casino",
                "Donus",
            };
            foreach ( var word in Food)
            {
                <p>@word</p>
            }
    }
</body>
</html>